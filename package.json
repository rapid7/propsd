{
  "name": "propsd",
  "version": "0.0.1",
  "description": "A local daemon for loading static and dynamic properties",
  "main": "lib/index.js",
  "directories": {
    "doc": "docs",
    "bin": "bin"
  },
  "scripts": {
    "start": "./bin/server.js",
    "test": "mocha",
    "lint": "eslint bin/** lib/** test/**",
    "coverage": "istanbul cover _mocha -- -R spec",
    "report-coverage": "npm run coverage && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "metadata-server": "./test/bin/metadata-server.js",
    "s3-server": "./test/bin/s3-server.js",
    "debug-server": "npm run metadata-server & npm run s3-server -- -d test/data/s3 & npm start -- -c config/dev.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rapid7/propsd.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rapid7/propsd/issues"
  },
  "homepage": "https://github.com/rapid7/propsd#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^5.2.0",
    "coveralls": "^2.11.8",
    "chokidar": "^1.4.3",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^3.1.0",
    "eslint-config-rapid7": "0.0.15",
    "eslint-plugin-rapid7": "^5.0.1",
    "eslint-plugin-react": "^3.14.0",
    "istanbul": "^0.4.2",
    "mocha": "^2.3.4",
    "mocha-lcov-reporter": "^1.2.0",
    "proxyquire": "^1.7.4",
    "rimraf": "^2.5.2",
    "s3rver": "0.0.13",
    "should": "^8.1.1",
    "should-sinon": "0.0.5",
    "sinon": "^1.17.3",
    "supertest": "^1.2.0",
    "walk": "^2.3.9"
  },
  "dependencies": {
    "aws-sdk": "^2.2.28",
    "consul": "^0.23.0",
    "express": "^4.13.3",
    "flat": "^2.0.0",
    "nconf": "^0.8.4",
    "winston": "^2.1.1",
    "yargs": "^4.1.0"
  }
}
