{
  "name": "propsd",
  "version": "3.1.3",
  "description": "A local daemon for loading static and dynamic properties",
  "scripts": {
    "start": "nodemon ./src/bin/server.js --exec babel-node --",
    "clean": "rimraf dist",
    "version": "./test/bin/version.js",
    "transpile": "npm run version && babel src --out-dir dist --copy-files",
    "test": "mocha --require @babel/register",
    "lint": "eslint src/ test/",
    "coverage": "nyc mocha --require @babel/register -- -R spec",
    "report-coverage": "npm run coverage && nyc report --reporter=text-lcov | coveralls",
    "metadata-server": "nodemon ./test/bin/metadata-server --",
    "s3-server": "node ./test/bin/s3-server.js",
    "dev-server": "npm run metadata-server & npm run s3-server -- -d test/data/s3 & npm start -- -c src/config/dev.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rapid7/propsd.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rapid7/propsd/issues"
  },
  "homepage": "https://github.com/rapid7/propsd#readme",
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
    "@babel/node": "7.8.7",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-decorators": "7.8.3",
    "@babel/plugin-proposal-export-namespace-from": "7.8.3",
    "@babel/plugin-proposal-function-sent": "7.8.3",
    "@babel/plugin-proposal-numeric-separator": "7.8.3",
    "@babel/plugin-proposal-throw-expressions": "7.8.3",
    "@babel/plugin-transform-runtime": "7.9.0",
    "@babel/preset-env": "7.9.0",
    "@babel/preset-stage-2": "7.8.3",
    "aws-sdk-mock": "5.1.0",
    "babel-eslint": "10.1.0",
    "chokidar": "3.3.1",
    "coveralls": "3.0.11",
    "eslint": "6.5.1",
    "eslint-config-rapid7": "2.11.1",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-react": "7.19.0",
    "mocha": "7.1.1",
    "mocha-lcov-reporter": "1.3.0",
    "nock": "12.0.3",
    "nodemon": "2.0.2",
    "nyc": "^15.0.0",
    "proxyquire": "2.1.3",
    "rimraf": "3.0.2",
    "s3rver": "3.5.0",
    "should": "13.2.3",
    "should-sinon": "0.0.6",
    "sinon": "9.0.1",
    "supertest": "4.0.2",
    "walk": "2.3.14"
  },
  "dependencies": {
    "@babel/runtime": "7.9.2",
    "aws-sdk": "2.635.0",
    "clone": "1.0.4",
    "consul": "0.23.0",
    "deep-diff": "0.3.8",
    "deep-equal": "1.0.1",
    "depd": "1.1.2",
    "express": "4.17.1",
    "express-winston": "1.3.1",
    "flat": "2.0.1",
    "immutable": "3.8.2",
    "lodash.isplainobject": "4.0.6",
    "nconf": "0.8.5",
    "winston": "2.1.1",
    "yargs": "4.1.0"
  }
}
