{
  "name": "propsd",
  "version": "3.0.3",
  "description": "A local daemon for loading static and dynamic properties",
  "scripts": {
    "start": "./bin/server.js -r babel-require",
    "clean": "rimraf dist",
    "transpile": "npm run clean && babel src --out-dir dist --copy-files && cp package.json dist",
    "test": "npm run transpile && mocha",
    "lint": "eslint src/** test/**",
    "coverage": "npm run transpile && istanbul cover _mocha -- -R spec",
    "report-coverage": "npm run coverage && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "metadata-server": "./test/bin/metadata-server",
    "s3-server": "./test/bin/s3-server.js",
    "dev-server": "npm run metadata-server & npm run s3-server -- -d test/data/s3 & npm start -- -c config/dev.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rapid7/propsd.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rapid7/propsd/issues"
  },
  "homepage": "https://github.com/rapid7/propsd#readme",
  "devDependencies": {
    "aws-sdk-mock": "~1.2.0",
    "babel-cli": "^6.24.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "~3.5.0",
    "chai-as-promised": "~5.2.0",
    "chokidar": "~1.4.3",
    "coveralls": "~2.11.8",
    "eslint": "~3.17.1",
    "eslint-config-rapid7": "~2.9.0",
    "eslint-plugin-import": "~2.2.0",
    "eslint-plugin-react": "~6.10.0",
    "istanbul": "~0.4.2",
    "mocha": "~3.3.0",
    "mocha-lcov-reporter": "~1.2.0",
    "nock": "~8.0.0",
    "proxyquire": "~1.7.4",
    "rimraf": "~2.5.2",
    "s3rver": "0.0.13",
    "should": "~8.1.1",
    "should-sinon": "0.0.5",
    "sinon": "~1.17.3",
    "supertest": "~1.2.0",
    "walk": "~2.3.9"
  },
  "dependencies": {
    "aws-sdk": "~2.2.28",
    "clone": "~1.0.2",
    "consul": "~0.23.0",
    "deep-diff": "~0.3.4",
    "deep-equal": "~1.0.1",
    "depd": "~1.1.0",
    "express": "~4.13.3",
    "express-winston": "~1.3.0",
    "flat": "~2.0.0",
    "immutable": "~3.8.1",
    "lodash.isplainobject": "^4.0.6",
    "nconf": "~0.8.4",
    "winston": "~2.1.1",
    "yargs": "~4.1.0"
  }
}
